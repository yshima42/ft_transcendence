{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:app": "cd ../ && ./start-app.sh",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write --loglevel=warn 'src/**/*.ts' './*.json'",
    "fix": "npm run --silent format; npm run --silent lint:fix",
    "test": "prisma migrate reset -f --skip-seed && jest --runInBand && prisma migrate reset -f",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "preinstall": "npx typesync || :",
    "prepare": "npx simple-git-hooks || :",
    "setup:dev": "bash ./setup-dev.sh",
    "db:seed:test:ua": "ts-node prisma/seed-cypress-user-account.ts"
  },
  "dependencies": {
    "@nestjs/cli": "^9.1.7",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/websockets": "^9.2.1",
    "@prisma/client": "^4.8.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/multer": "^1.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-42": "^1.2.6",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "socket.io": "^4.5.4",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.2.2",
    "@nestjs/testing": "^9.2.1",
    "@prisma/generator-helper": "^4.8.0",
    "@types/eslint": "^8.4.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "29.2.4",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/prettier": "^2.7.2",
    "@types/qrcode": "^1.5.0",
    "@types/rimraf": "^3.0.2",
    "@types/socket.io": "^3.0.2",
    "@types/source-map-support": "^0.5.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cookie": "^0.5.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "prisma-class-validator-generator": "^0.2.0",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-docs-generator": "^0.5.0",
    "prisma-erd-generator": "^1.2.4",
    "simple-git-hooks": "^2.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "tsconfig-paths": "4.1.1",
    "typescript": "*",
    "webpack": "^5.75.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write --loglevel=warn",
      "eslint --fix --quiet"
    ],
    "src/**/*.{css,less,sass,scss}": [
      "stylelint --fix --quiet"
    ],
    "src/**/*.{html,json,gql,graphql}": [
      "prettier --write --loglevel=error"
    ],
    "./*.json": [
      "prettier --write --loglevel=error"
    ],
    "prisma/schema.prisma": [
      "prisma format"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./lint-staged-each.sh",
    "post-checkout": "./setup-dev.sh",
    "post-merge": "./setup-dev.sh",
    "pre-push": "./check-conflict.sh"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.+)": "<rootDir>/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}

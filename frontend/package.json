{
  "name": "ft_transcendence",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:dev": "vite",
    "start:app": "cd ../ && ./start-app.sh",
    "api": "yarn --cwd ../backend start:dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint 'src/**/*.{css,less,sass,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{css,less,sass,scss}'",
    "lint": "npm run --silent lint:style; npm run --silent lint:es",
    "lint:fix": "npm run --silent lint:style:fix; npm run --silent lint:es:fix",
    "preinstall": "npx typesync || :",
    "format": "prettier --write --loglevel=warn 'src/**/*.{js,jsx,ts,tsx,html,json,gql,graphql}' './*.json'",
    "fix": "npm run --silent format; npm run --silent lint:fix",
    "prepare": "npx simple-git-hooks || :",
    "test": "jest --config ./jest.config.json",
    "test:watch": "npm run test -- --watch",
    "storybook": "export NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "db:pull": "dotenv -e .env.development -- yarn prisma db pull",
    "setup:dev": "yarn install && cp ../backend/prisma/schema.prisma ./prisma && yarn prisma generate"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.4.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@tanstack/react-query": "^4.19.1",
    "axios": "^1.2.0",
    "framer-motion": "^7.6.18",
    "next-qrcode": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.4.4",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@prisma/client": "^4.7.1",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7.1.20",
    "@types/chai": "^4.3.4",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.2.4",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^9.1.0",
    "chai": "^4.3.7",
    "cypress": "^11.2.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.5.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "npm-check-updates": "^16.4.3",
    "prettier": "^2.8.0",
    "prisma": "^4.7.1",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "*",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^3.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write --loglevel=warn",
      "eslint --fix --quiet"
    ],
    "src/**/*.{css,less,sass,scss}": [
      "stylelint --fix --quiet"
    ],
    "src/**/*.{html,json,gql,graphql}": [
      "prettier --write --loglevel=error"
    ],
    "./*.json": [
      "prettier --write --loglevel=error"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./lint-staged-each.sh",
    "post-checkout": "./setup-dev.sh",
    "post-merge": "./setup-dev.sh",
    "pre-push": "./check-conflict.sh && ./check-package-latest.sh"
  }
}

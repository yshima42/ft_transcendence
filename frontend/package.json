{
  "name": "ft_transcendence",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:dev": "vite",
    "api": "yarn --cwd ../backend start:dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:es": "eslint'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint--fix'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint'src/**/*.{css,less,sass,scss}'",
    "lint:style:fix": "stylelint--fix'src/**/*.{css,less,sass,scss}'",
    "lint": "npmrun--silentlint:style;npmrun--silentlint:es",
    "lint:fix": "npmrun--silentlint:style:fix;npmrun--silentlint:es:fix",
    "preinstall": "npx typesync || :",
    "format": "prettier --write --loglevel=warn 'src/**/*.{js,jsx,ts,tsx,html,json,gql,graphql}'",
    "fix": "npm run --silent format; npm run --silent lint:fix",
    "prepare": "npx simple-git-hooks || :",
    "test": "jest --config ./jest.config.json",
    "test:watch": "npm run test -- --watch",
    "storybook": "export NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "axios": "^1.1.3",
    "framer-motion": "^7.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@prisma/client": "^4.6.1",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.9",
    "@types/jest": "^29.2.2",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.3.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^14.14.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "*",
    "vite": "^3.2.1",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write --loglevel=warn",
      "eslint --fix --quiet"
    ],
    "src/**/*.{css,less,sass,scss}": [
      "stylelint --fix --quiet"
    ],
    "src/**/*.{html,json,gql,graphql}": [
      "prettier --write --loglevel=error"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./lint-staged-each.sh"
  }
}
